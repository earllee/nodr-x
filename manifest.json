{
  "manifest_version": 2,

  "name": "Nodr-x",
  "description": "This extension records a browsing session.",
  "version": "1.0",
  "background": { "scripts": ["background.js"]},
  "content_scripts": [ 
	{
    "matches": ["*://*"], "js": ["content_scripts.js"]
	}
  ],
  "permissions": [
    "history", "tabs", "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
