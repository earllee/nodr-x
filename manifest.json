{
  "manifest_version": 2,

  "name": "Nodr-x",
  "description": "This extension records a browsing session.",
  "version": "1.0",
  "background": { "page" : "background.html"},
  "content_scripts": [ 
	{
    "matches": ["http://*/*","https://*/*"], "js": ["jquery-2.0.3.js", "content_scripts.js"]
	}
  ],
  "permissions": [
    "history", "tabs", "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
