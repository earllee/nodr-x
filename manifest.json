{
  "manifest_version": 2,

  "name": "Nodr-x",
  "description": "This extension records a browsing session.",
  "version": "1.0",
  "background": { "scripts": ["background.js"]},
  "content_scripts": [ 
	{
	"matches": ["http://*/*"], "js": ["tracking.js"]
	}
  ],
//  "web_accessible_resources": [
//   "jquery-2.0.2.min.map"
//  ],
  "permissions": [
    "history", "tabs", "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
