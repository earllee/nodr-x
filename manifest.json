{
  "manifest_version": 2,

  "name": "Nodr-x",
  "description": "This extension records a browsing session.",
  "version": "1.0",
  "background": { "scripts": ["background.js"]},
  "content_scripts": [ 
	{
	"matches": ["http://*/*"], "js": ["jquery.js", "tracking.js"]
	}
  ],
  "permissions": [
    "history", "tabs", "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
